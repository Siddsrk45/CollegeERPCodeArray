<!DOCTYPE html>
@model CollegeERP.Models.Student
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
      @*@ViewBag.datas*@
        @*@{ 
            foreach (var item in ViewBag.datas)
            {
                @item.Name
            }
        }*@
        <table>
            <tr>
                <th> Name </th>
                <th> Middlename </th>
                <th> Last Name </th>
                <th>Date of Birth</th>
                <th>Gender</th>
            </tr>
            @foreach (var item in ViewBag.datas)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Middlename</td>
                    <td>@item.LastName</td>
                    <td> @Html.EditorFor(x => x.DOB)</td>
                    <td><select>
    <option value="1">Male</option>
    <option value="2">Female</option>
    <option value="3">Other </option>
</select></td>
                </tr>
            }



        </table>
    </div>
</body>
</html>
